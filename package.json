{
  "name": "my-app-final-light",
  "version": "1.0.0",
  "description": "Musterimplementation der NestJS-Anwendung myApp",
  "author": "Dirk Decher",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "nest start --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:cov": "jest --config jest.config.ts --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "docs:delete-old-files": "node scripts/clean-dir.mjs docs",
    "docs:openapi": "ts-node -r tsconfig-paths/register scripts/main-swagger-export.ts",
    "docs:erd:per-module": "node scripts/generate-erd-per-module.mjs",
    "docs:readmes": "node scripts/generate-module-readmes.mjs",
    "docs:pdf": "node scripts/generate-pdf-from-readme-md.mjs",
    "docs:generate": "pnpm docs:openapi && pnpm docs:erd:per-module && pnpm docs:readmes",
    "compo-doc:delete-old-files": "node scripts/clean-dir.mjs documentation",
    "compo-doc": "pnpm compo-doc:delete-old-files && compodoc -c compodoc.json",
    "all-doc": "pnpm docs:delete-old-files && pnpm docs:openapi && pnpm docs:erd:per-module && pnpm docs:generate && pnpm docs:pdf && pnpm compo-doc"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.12",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.12",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^11.1.12",
    "@nestjs/swagger": "^11.2.5",
    "@nestjs/typeorm": "^11.0.0",
    "argon2": "^0.44.0",
    "class-transformer": "^0.5.1",
    "dotenv": "^17.2.3",
    "pg": "^8.17.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.28",
    "yaml": "^2.8.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.2.1",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@mermaid-js/mermaid-cli": "^11.12.0",
    "@nestjs/cli": "^11.0.16",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.12",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.19.7",
    "@types/supertest": "^6.0.3",
    "class-validator": "^0.14.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "fast-glob": "^3.3.3",
    "globals": "^16.5.0",
    "jest": "^29.7.0",
    "markdown-it": "^14.1.0",
    "mermaid": "^11.12.2",
    "only-allow": "^1.2.2",
    "prettier": "^3.8.0",
    "rimraf": "^5.0.10",
    "source-map-support": "^0.5.21",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "ts-morph": "^27.0.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0"
  }
}
